{
  "name": "tachyons-webpack",
  "version": "1.1.2",
  "description": "A boilerplate for using tachyons with webpack",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run browsersync",
    "build": "webpack --config webpack.config.js --debug --progress && rm ./dist/tachyons.js",
    "clean": "rm -rf ./dist",
    "browsersync": "node bin/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tachyons-css/tachyons-webpack.git"
  },
  "keywords": [
    "tachyons",
    "webpack",
    "boilerplate"
  ],
  "author": "Garren Smith <garren@redcometlabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tachyons-css/tachyons-webpack/issues"
  },
  "homepage": "https://github.com/tachyons-css/tachyons-webpack#readme",
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "browser-sync": "^2.17.5",
    "bs-fullscreen-message": "^1.1.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "strip-ansi": "^4.0.0",
    "style-loader": "^0.18.2",
    "tachyons": "4.8.0",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.7.1"
  }
}
